\par {\listtext\pard\plain\cs20\f3\fs20\lang2057\cgrid \loch\af3\dbch\af0\hich\f3 \'b7\tab}}\pard \fi-360\li720\nowidctlpar\widctlpar\jclisttab\tx720\ls186\adjustright {\cs20\b\uldb Import of dBase files}{\cs21\v ID_ImportDBase
